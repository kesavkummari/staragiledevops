node {
    stage('Clean'){
        sh 'mvn clean'
    }
    stage('Validate'){
        sh 'mvn validate'
    }
    stage('Compile'){
        sh 'mvn compile'
    }
    stage('Test'){
        sh 'mvn test -DskipTests'
    }
    stage('Install'){
        sh 'mvn install -DskipTests'
    }
    stage('Verify'){
        sh 'mvn verify -DskipTests'
    }
}
